.pannel-item {
    display: flex;
    align-items: center;
    height: 46px;
    padding: 0 32rpx;
    font-size: 32rpx;
    background-color: #fff;
  }
  
  .pannel-label {
    width: 160rpx;
    margin-right: 32rpx;
  }
  
  .pannel-text {
    flex: 1;
    color: #000;
    opacity: 0.9;
  }
  .pannel-text.empty {
    opacity: 0.26;
  }