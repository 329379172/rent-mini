.card {
    width: 100%;
    display: flex;
    flex-direction: column;
    background: #fff;
    padding: 10px;
    box-sizing: border-box;
}

Page {
    background: #f8f6fc;
}

.card-container {
    width: 100%;
    box-sizing: border-box;
    display: flex;
    flex-wrap: wrap;
    margin-top: 15px;
}

.item {
    width: 33.3%; display: flex; flex-direction: column; align-items: center;
    color: #333;
    font-size: 14px;
}