.container {
  width: 100%;
  height: 100%;
  display: block;
  margin: 0;
  padding: 0;
}

.tab-list {
  display: flex;
  flex-direction: row;
  margin-top: 20px;
}

.tab-list-item {
  flex: 1;
  text-align: center;
}

.search-label {
  display: flex;
  flex-direction: row;
  align-items: center;
  margin-right: 10px;
  margin-left: 10px;
}

.triangle {
  width: 0;
  height: 0;
  border: 3px solid transparent;
  border-top-color: black ;
  margin-left: 5px;
}

#cont {
  height: 1px;
}

.filter-list-fixed {
  display: flex;
  flex-direction: row;
  background-color: #fff;
  color: #333333;
  border-bottom-style: solid;
  border-bottom-width: 1px;
  border-bottom-color: #eeeeee;
  height: 40px;
  font-size: 16px;
  top: 0;
}

.filter-list {
  display: flex;
  flex-direction: row;
  background-color: #fff;
  z-index: 1000;
  color: #333333;
  border-bottom-style: solid;
  border-bottom-width: 1px;
  border-bottom-color: #eeeeee;
  height: 40px;
  margin-top: 5px;
  margin-left: 10px;
  margin-right: 10px;
  font-size: 16px;
}

.activeD {
  background: #eff0f1;
}

.filter-list-item {
  flex: 1;
  text-align: center;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}

.filter-list-item-last {
  text-align: center;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.house-list {
  display: flex;
  margin: 50px 10px 0 10px;
  flex-direction: column;
  padding-bottom: calc(safe-area-intent-bottom + 50);
}

.house-list-item {
  display: flex;
  flex-direction: row;
  margin-bottom: 20px;
}

.house-list-item-icon {
}

.house-list-item-right {
  display: flex;
  flex-direction: column;
  margin-left: 20px;
}

.house-list-item-tag {
  display: flex;
  flex-direction: row;
  font-size: 11px;
  margin-top: 5px;
  margin-bottom: 5px;
}

.house-list-item-intro {
  font-size: 11px;
  margin-top: 5px;
}

.house-list-item-tag-item {
  background: #eef0f3;
  margin-right: 10px;
  color: #8899ac;
}

.house-list-item-title {
  color: #222b42;
  font-size: 17px;
}

.house-list-item-price {
  color: #e7634c;
  display: flex;
  flex-direction: row;
  align-items: flex-end;
  font-weight: bold;
}

.filter-context {
}

.hide {
  display: none !important;
}

.whitebg {
  background-color: #fff;
}

.filter-context-d {
  display: flex;
  flex-direction: column;
  max-height: 400px;
  overflow-y: scroll;
}

.filter-context-d-root {
  width: 100px;
  display: flex;
  flex-direction: column;
  align-items: center;
  max-height: 400px;
  height: 400px;
  overflow-y: scroll;
}

.filter-context-d-root view {
  height: 40px;
  font-size: 16px;
  line-height: 40px;
}

.filter-context-d-2 {
  display: flex;
  flex: 1;
  flex-direction: column;
  align-items: center;
  background-color: #f7f8f9;
  overflow-y: scroll;
}

.filter-context-d-2 view {
  font-size: 16px;
  line-height: 40px;
  text-align: center;
}

.filter-context-d-3 {
  display: flex;
  flex: 1;
  flex-direction: column;
  align-items: center;
  background-color: #fff;
  padding: 0 20px;
  max-height: 400px;
  height: 400px;
  overflow-y: scroll;
}

.filter-context-d-3 view {
  height: 40px;
  font-size: 16px;
  line-height: 40px;
}

.content-item-class	 {
  width: 100%;
}

.sumbitter {
  display: flex;
  flex-direction: row;
  padding: 10px 20px;
  margin-top: 20px;
}

.reset {
  display: flex;
  flex: 1;
  background: #fff;
  padding: 10px;
  margin-right: 20px;
  justify-content: center;
  border-radius: 5px;
}

.look {
  display: flex;
  flex: 1;
  padding: 10px;
  margin-left: 20px;
  justify-content: center;
  background: #00a962;
  color: #fff;
}

.all-rent-item {
  flex: 1;
  margin: 0 10px;
  box-sizing: border-box;
  background: #fff;
  align-items: center;
  text-align: center;
  padding: 5px 0;
  margin-top: 10px;
}

.all-rent-item-checked {
  flex: 1;
  margin: 0 10px;
  box-sizing: border-box;
  align-items: center;
  text-align: center;
  padding: 5px 0;
  margin-top: 10px;
  color: #fff;
  background: red;
}

.price-item {
  padding: 5px;
}

.checked {
  color: red
}